{
  "_version": "1.59.0",
  "sap.app": {
    "id": "sap.btp.blogdetail",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:feop",
      "version": "1.14.2",
      "toolsId": "b05d208a-1dd4-4da0-ae81-635150ab6852"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/blog/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "flexibilityServices": [
      {
          "layer": "VENDOR",
          "connector": "KeyUserConnector",
          "url": "/sap/bc/lrep"
      },
      {
          "layer": "USER",
          "connector": "NavigationConnector",
          "url": "/servicebrokerapi/sap/v2/url"
      }
    ],  
    "dependencies": {
      "minUI5Version": "^1.120",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "rootView": {
      "viewName": "sap.btp.blogdetail.view.DetailView",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sap.btp.blogdetail.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        },
        "type": "sap.ui.model.json.JSONModel"
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "async": true,
        "viewType": "XML",
        "viewPath": "sap.btp.blogdetail.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": "notFound"
        },
        "path": "sap.btp.blogdetail.view"
      },
      "routes": [
        {
          "pattern": "Blogs({key}):?query:",
          "name": "BlogsObjectPage",
          "target": "BlogsObjectPage"
        },
        {
          "pattern": "Blogs({key})/categories({key2}):?query:",
          "name": "BlogCategoriesObjectPage",
          "target": "BlogCategoriesObjectPage"
        },
        {
          "pattern": "",
          "name": "DetailView",
          "target": "DetailView"
        }
      ],
      "targets": {
        "DetailView": {
          "viewName": "DetailView",
          "viewLevel": 1
        },
        "BlogsObjectPage": {
          "type": "Component",
          "id": "BlogsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Blogs",
              "content": {
                "header": {
                  "visible": false,
                  "anchorBarVisible": false
                }
              },
              "navigation": {
                "categories": {
                  "detail": {
                    "route": "BlogCategoriesObjectPage"
                  }
                }
              }
            }
          }
        },
        "BlogCategoriesObjectPage": {
          "type": "Component",
          "id": "BlogCategoriesObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Blogs/categories"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
