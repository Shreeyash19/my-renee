{
  "name": "my-renee",
  "version": "1.0.0",
  "description": "Renee, lessons learned in implementation projects",
  "repository": "https://github.com/Shreeyash19/my-renee/tree/deploy09082024",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap/approuter": "^16.7.3",
    "@sap/cds": "^8.2.1",
    "@sap/xssec": "^4",
    "@ui5/cli": "^4.0.6",
    "express": "^4",
    "ui5-task-zipper": "^3.3.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1.7.3",
    "@sap/cds-dk": "^8.2.0",
    "@sap/eslint-plugin-cds": "^3.0.4",
    "@sap/ux-specification": "^1.124.0",
    "cds-plugin-ui5": "^0.11.0",
    "eslint": "^9.10.0",
    "rimraf": "^6.0.1"
  },
  "scripts": {
    "build": "rimraf resources mta_archives && mbt build -t gen --mtar mta.tar",
    "debug": "node --inspect-brk ./node_modules/@sap/cds/bin/cds-serve.js --in-memory?",
    "predeploy": "npm install && npm run build",
    "deploy": "cf deploy gen/mta.tar --retries 1",
    "start": "cds-serve",
    "undeploy": "cf undeploy my-renee --delete-services --delete-service-keys --delete-service-brokers",
    "watch-categories": "cds watch --open categories/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-classifications": "cds watch --open classifications/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-configurations": "cds watch --open configurations/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-curatorlessons": "cds watch --open curatorlessons/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-mylessons": "cds watch --open mylessons/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-personas": "cds watch --open personas/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-scopeitems": "cds watch --open scopeitems/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-sourcetypes": "cds watch --open sourcetypes/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-status": "cds watch --open status/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-teams": "cds watch --open teams/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-user": "cds watch --open user/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-versions": "cds watch --open versions/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch": "nodemon -e cds,js,json,properties --exec cds run"
  },
  "sapux": [
    "app/categories",
    "app/classifications",
    "app/configurations",
    "app/curatorlessons",
    "app/mylessons",
    "app/personas",
    "app/scopeitems",
    "app/sourcetypes",
    "app/status",
    "app/teams",
    "app/user",
    "app/versions"
  ],
  "workspaces": [
    "app/*"
  ],
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa",
        "db": "hana"
      },
      "auth": {
        "kind": "basic",
        "users": {
          "admin@sap.com": {
            "password": "W@lcome2024!",
            "roles": [
              "renee-Administrator"
            ],
            "scopes": [
              "Administer",
              "Curate",
              "Author",
              "View"
            ]
          },
          "curator@sap.com": {
            "password": "W@lcome2024!",
            "roles": [
              "renee-Curator"
            ],
            "scopes": [
              "Curate",
              "Author",
              "View"
            ]
          },
          "author@sap.com": {
            "password": "W@lcome2024!",
            "roles": [
              "renee-Author"
            ],
            "scopes": [
              "Author",
              "View"
            ]
          },
          "*": false
        }
      },
      "destinations": true,
      "html5-repo": true,
      "workzone": true
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
